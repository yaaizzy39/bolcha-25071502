# Rename this file to `.env` and fill in your Firebase project values
# These keys can be found in your Firebase console → Project settings → General → Your apps → Firebase SDK snippet (config)

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Add your Google Apps Script endpoint URL here
# You can add multiple endpoints separated by commas
VITE_GAS_ENDPOINTS=your_gas_endpoint_url_here

# Admin password for accessing admin panel
VITE_ADMIN_PASSWORD=your_admin_password_here
